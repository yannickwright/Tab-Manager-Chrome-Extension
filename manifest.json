{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.3.7",
  "description": "Organize and manage your browser tabs efficiently",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Manager"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}

